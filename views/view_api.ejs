<%- include('header') -%>

<section class="section pb-0">
    <div class="container">
      <div class="row">


        <div class="col-lg-12 mb-5">
          <h2 class="h5 section-title">Trending Post</h2>
          <div class="row g-3">

            <% API_data.forEach(function(res) { %>
              <div class="col-lg- col-md-6 col-12">
              <article class="card mb-4">
                <div class="card-body d-flex">
                  <div class="ml-3">
                    <h4><a href="view_api/<%= res.id%>" class="post-title"><%= res.name %></a></h4>
                  </div>
                </div>
              </article>
              </div>
            <% }); %>
          </div>
          
        </div>
        
      
        <div class="col-12">
          <div class="border-bottom border-default"></div>
        </div>
      </div>
    </div>
  </section>

<%- include('footer') -%>
